{
  "name": "pandoc-web",
  "version": "1.0.0",
  "dependencies": {
    "@mdi/font": "^7.2.96",
    "express": "^4.18.2",
    "roboto-fontface": "^0.10.0",
    "vue": "^3.3.4",
    "vuetify": "^3.3.14"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.11",
    "@babel/node": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@vitejs/plugin-vue": "^4.3.3",
    "autoprefixer": "^10.4.15",
    "concurrently": "^8.2.1",
    "postcss": "^8.4.28",
    "prettier": "^3.0.2",
    "tailwindcss": "^3.3.3",
    "vite": "^4.4.9",
    "vite-plugin-vuetify": "^1.0.2"
